                                <p>
                                    Tüm Başlıklar
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Login/CikisYap" class="nav-link">
                                <i class="nav-icon far fa-image"></i>
                                <p>
                                    Çıkış Yap
                                </p>
                            </a>
